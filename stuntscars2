    <!DOCTYPE html>
<html lang="en-us">
<head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <link rel="stylesheet" href="https://rawcdn.githack.com/IdkDwij/The-Hamster-Calculator/5e20012b9adc78dd9ba97d90b0c556eafcf7562d/public/mcm-webgl/mcm-webgl/TemplateData/style.css">
    <style>
        #unity-container { width: 100%; height: 100%; overflow: hidden; position: absolute; }
        #unity-canvas { width: 100%; height: 100%; background: #231f20; }
    </style>
</head>
<body>
    <div id="unity-container">
        <canvas id="unity-canvas"></canvas>
        <div id="unity-loading-bar" style="display: block;">
            <div id="unity-progress-bar-empty">
                <div id="unity-progress-bar-full"></div>
            </div>
        </div>
    </div>
    <script>
        var buildUrl = "https://rawcdn.githack.com/IdkDwij/The-Hamster-Calculator/5e20012b9adc78dd9ba97d90b0c556eafcf7562d/public/mcm-webgl/mcm-webgl/Build";
        var loaderUrl = buildUrl + "/304bc71d77b7acd6469bb5cc0730073b.js";
        var config = {
            dataUrl: buildUrl + "/97a54d9b4b7525d0f2328e0cbb512980.data",
            frameworkUrl: buildUrl + "/4e20d7eb8e868230cafe4baff2e177d4.js",
            codeUrl: buildUrl + "/03cde5b9e5b1a5b23fbab20d045d4958.wasm",
            streamingAssetsUrl: "https://rawcdn.githack.com/IdkDwij/The-Hamster-Calculator/5e20012b9adc78dd9ba97d90b0c556eafcf7562d/public/mcm-webgl/mcm-webgl/StreamingAssets",
            companyName: "Madalin Games",
            productName: "Madalin Cars Multiplayer",
            productVersion: "2.0.0",
        };

        var canvas = document.querySelector("#unity-canvas");
        var loadingBar = document.querySelector("#unity-loading-bar");
        var progressBarFull = document.querySelector("#unity-progress-bar-full");

        var script = document.createElement("script");
        script.src = loaderUrl;
        script.onload = () => {
            createUnityInstance(canvas, config, (progress) => {
                progressBarFull.style.width = 100 * progress + "%";
            }).then((unityInstance) => {
                loadingBar.style.display = "none";
            }).catch((message) => {
                alert(message);
            });
        };
        document.body.appendChild(script);
    </script>
